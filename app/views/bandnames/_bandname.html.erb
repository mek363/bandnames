<tr>
  <th scope="row">
    <%= link_to bandname.name, bandname %> 
  </th>
  <td>
    <%= number_with_precision(bandname.cached_weighted_average, precision: 2) %>
    <% bandname.rating.to_i.times do  %>
      <%= render 'layouts/voteup' %>
    <% end %>
    <% bandname.blank_stars.times do %>
      <%= render 'layouts/votedn' %>
    <% end %>
    (<%= bandname.cached_votes_total %>)
  </td>
</tr>